<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="report_vehicle_rent" >
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <h2 align="center">Vehicle Rental</h2>
                <div class="page">
                    <div class="oe_structure"/>
                    <div class="row">


                        <table class="table table-condensed">
                            <head>
                                <tr>
                                    <th style="text-align:left;"><strong>Vehicle:</strong></th>
                                    <th style="text-align:left;"><strong>From Date:</strong></th>
                                    <th style="text-align:left;"><strong>To Date:</strong></th>
                                </tr>
                            </head>
                            <body>
                                <t t-foreach="docs" t-as="o">
                                    <tr>
                                        <td style="text-align:left;">
                                            <t t-esc="o['vehicle_id']"/>
                                        </td>
                                        <td style="text-align:left;">
                                            <t t-esc="o['from_date']"/>
                                        </td>
                                        <td style="text-align:left;">
                                            <t t-esc="o['to_date']"/>
                                        </td>
                                    </tr>
                                </t>
                            </body>
                        </table>
                    </div>
                </div>
            </t>
        </t>
    </template>

</odoo>